{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/validate-env.js && vite build",
    "build:docker": "sh scripts/docker-build.sh",
    "build:staging": "node scripts/validate-env.js && tsc -b && vite build --mode staging",
    "build:production": "node scripts/validate-env.js && tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:auth": "playwright test auth.spec.ts",
    "test:e2e:customer": "playwright test customer.spec.ts",
    "test:e2e:mobile": "playwright test mobile.spec.ts",
    "test:e2e:i18n": "playwright test localization.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src",
    "validate": "node scripts/validate-frontend.js"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/react": "^10.0.0",
    "@sentry/tracing": "^7.120.4",
    "@types/google.maps": "^3.58.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-signature-canvas": "^1.0.7",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "antd": "^5.26.5",
    "axios": "^1.10.0",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "eventemitter3": "^5.0.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "idb": "^8.0.3",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "react-router-dom": "^7.7.0",
    "react-signature-canvas": "^1.0.7",
    "recharts": "^3.1.0",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-compression": "^0.5.1"
  }
}

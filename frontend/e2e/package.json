{
  "name": "e2e",
  "version": "1.0.0",
  "description": "This directory contains end-to-end tests for the Lucky Gas frontend application using Playwright.",
  "main": "dist/setup/test-server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "build:server": "tsc --project tsconfig.server.json",
    "start:server": "npm run build:server && node dist/setup/test-server.js",
    "start:mock": "node mock-backend.js",
    "dev:server": "tsx watch setup/test-server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}
